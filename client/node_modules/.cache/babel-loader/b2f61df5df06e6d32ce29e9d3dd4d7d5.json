{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      train_data: JSON.parse(this.$route.params.train_data),\n      count: 0,\n      name: '',\n      gender: '',\n      berth: '',\n      passenger_details: []\n    };\n  },\n\n  methods: {\n    async bookTicket() {\n      if (this.train_data.availability < this.count) {\n        this.count = 0;\n        alert(\"seats not available\");\n        return;\n      }\n\n      console.log(this.passenger_details);\n      const result = await axios.post('http://localhost:8081/ticket', this.passenger_details);\n      console.log(result);\n\n      if (result.data.status === 'success') {\n        alert('Ticket confirmed');\n      }\n    },\n\n    Dummy(passenger) {\n      const obj = {};\n      obj.train_no = this.train_data.train_no;\n      obj.name = passenger.name;\n      obj.gender = passenger.gender;\n      obj.berth = passenger.berth;\n      return obj;\n    }\n\n  },\n  watch: {\n    count: function (value) {\n      console.log('called');\n      this.passenger_details = [];\n\n      for (let i = 0; i < value; i++) {\n        this.passenger_details.push(this.Dummy({\n          name: '',\n          gender: '',\n          berth: ''\n        }));\n      }\n\n      console.log(this.passenger_details);\n      console.log('passenger created');\n    }\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,GAAE;AACF,WAAO;AACHC,gBAAU,EAACC,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,MAAZ,CAAmBJ,UAA9B,CADR;AAEHK,WAAK,EAAC,CAFH;AAGHC,UAAI,EAAC,EAHF;AAIHC,YAAM,EAAC,EAJJ;AAKHC,WAAK,EAAC,EALH;AAMHC,uBAAiB,EAAC;AANf,KAAP;AAQH,GAVU;;AAWXC,SAAO,EAAC;AACJ,UAAMC,UAAN,GAAkB;AACd,UAAG,KAAKX,UAAL,CAAgBY,YAAhB,GAA6B,KAAKP,KAArC,EAA2C;AACvC,aAAKA,KAAL,GAAW,CAAX;AACAQ,aAAK,CAAC,qBAAD,CAAL;AACA;AACJ;;AACAC,aAAO,CAACC,GAAR,CAAY,KAAKN,iBAAjB;AACA,YAAMO,MAAM,GAAC,MAAMlB,KAAK,CAACmB,IAAN,CAAW,8BAAX,EAA0C,KAAKR,iBAA/C,CAAnB;AACAK,aAAO,CAACC,GAAR,CAAYC,MAAZ;;AACA,UAAGA,MAAM,CAACjB,I