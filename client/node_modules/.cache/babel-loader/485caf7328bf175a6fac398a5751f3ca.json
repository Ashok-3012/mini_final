{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      user: {\n        name: '',\n        user_id: '',\n        pass: '',\n        cpass: '',\n        dob: '',\n        gender: '',\n        contact: '',\n        email_id: ''\n      },\n      status: false,\n      p_status: false\n    };\n  },\n\n  methods: {\n    async signUp() {\n      if (this.user.pass !== this.user.cpass) {\n        this.p_status = true;\n        setTimeout(() => {\n          this.p_status = false;\n        }, 3000);\n        return;\n      }\n\n      const res = await axios.post('http://localhost:8081/sign_up', this.user);\n      console.log(res.data.status);\n      if (res.data.status === 'failure') this.status = true;else this.$router.push({\n        name: 'login'\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,GAAE;AACF,WAAO;AACHC,UAAI,EAAC;AACDC,YAAI,EAAC,EADJ;AAEDC,eAAO,EAAC,EAFP;AAGDC,YAAI,EAAC,EAHJ;AAIDC,aAAK,EAAC,EAJL;AAKDC,WAAG,EAAC,EALH;AAMDC,cAAM,EAAC,EANN;AAODC,eAAO,EAAC,EAPP;AAQDC,gBAAQ,EAAC;AARR,OADF;AAWHC,YAAM,EAAC,KAXJ;AAYHC,cAAQ,EAAC;AAZN,KAAP;AAcH,GAhBU;;AAiBXC,SAAO,EAAC;AACJ,UAAMC,MAAN,GAAc;AACV,UAAG,KAAKZ,IAAL,CAAUG,IAAV,KAAiB,KAAKH,IAAL,CAAUI,KAA9B,EAAqC;AACjC,aAAKM,QAAL,GAAc,IAAd;AACAG,kBAAU,CAAC,MAAI;AACX,eAAKH,QAAL,GAAc,KAAd;AACH,SAFS,EAER,IAFQ,CAAV;AAGA;AACJ;;AACA,YAAMI,GAAG,GAAC,MAAMhB,KAAK,CAACiB,IAAN,CAAW,+BAAX,EAA2C,KAAKf,IAAhD,CAAhB;AACAgB,aAAO,CAACC,GAAR,CAAYH,GAAG,CAACf,IAAJ,CAASU,MAArB;AACA,UAAGK,GAAG,CAACf,IAAJ,CAASU,MAAT,KAAkB,SAArB,EAAgC,KAAKA,MAAL,GAAY,IAAZ,CAAhC,KACK,KAAKS,OAAL,CAAaC,IAAb,CAAkB;AAAClB,YAAI,EAAC;AAAN,OAAlB;AACT;;AAbI;AAjBG,CAAf","names":["axios","data","user","name","user_id","pass","cpass","dob","gender","contact","email_id","status","p_status","methods","signUp","setTimeout","res","post","console","log","$router","push"],"sourceRoot":"","sources":["/Users/ecom-ashok.bs/Desktop/mini-project/client/src/views/SignUp.vue"],"sourcesContent":["<template>\n  <div>\n      <h1>Welcome to Indian Railways!</h1>\n      \n      <input type='text' placeholder=\"Enter the name\" v-model=\"user.name\"/>\n      <input type='text' placeholder=\"Enter the user_id\" v-model=\"user.user_id\"/>\n      <input type='text' placeholder=\"Enter the password\" v-model=\"user.pass\"/>\n      <input type='text' placeholder=\"Re-Enter the password\" v-model=\"user.cpass\"/>\n      <input type='text' placeholder=\"Enter the date of birth\" v-model=\"user.dob\"/>\n      <input type='text' placeholder=\"Enter the gender\" v-model=\"user.gender\"/>\n      <input type='number' placeholder=\"Enter the contact number\" v-model=\"user.contact\"/>\n      <input type='tel' placeholder=\"Enter the email_id\" v-model=\"user.email_id\"/>\n      <button @click='signUp'>Create Account</button>\n      <h1 v-show=\"status\">User Already Exists!Please Login</h1>\n      <h1 v-show=\"p_status\">Password and Confirm Password Doesn't match</h1>\n      \n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n    data(){\n        return {\n            user:{\n                name:'',\n                user_id:'',\n                pass:'',\n                cpass:'',\n                dob:'',\n                gender:'',\n                contact:'',\n                email_id:'',\n            },\n            status:false,\n            p_status:false\n        }\n    },\n    methods:{\n        async signUp(){\n            if(this.user.pass!==this.user.cpass) {\n                this.p_status=true;\n                setTimeout(()=>{\n                    this.p_status=false;\n                },3000);\n                return;\n            }\n            const res=await axios.post('http://localhost:8081/sign_up',this.user);\n            console.log(res.data.status);\n            if(res.data.status==='failure') this.status=true;\n            else this.$router.push({name:'login'});\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]},