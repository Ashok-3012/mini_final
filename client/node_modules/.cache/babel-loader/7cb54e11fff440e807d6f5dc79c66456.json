{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      train_data: JSON.parse(this.$route.params.train_data),\n      count: 0,\n      DOJ: '',\n      name: '',\n      gender: '',\n      berth: '',\n      mail_id: '',\n      train_no: '',\n      train_name: '',\n      // src:'',\n      //dest:'',\n      passenger_details: []\n    };\n  },\n\n  methods: {\n    async bookTicket() {\n      if (this.train_data.availability < this.count) {\n        this.count = 0;\n        alert(\"seats not available\");\n        return;\n      }\n\n      console.log(this.passenger_details);\n      const result = await axios.post('http://localhost:8081/ticket', this.passenger_details);\n      console.log(result);\n\n      if (result.data.status === 'success') {\n        alert('Ticket confirmed');\n      }\n    },\n\n    Dummy(passenger) {\n      const obj = {};\n      obj.train_no = this.train_data.train_no;\n      obj.train_name = this.train_data.train_name;\n      obj.doj = this.DOJ; //obj.src=this.place.src;\n      //obj.dest=this.place.dest;\n\n      obj.name = passenger.name;\n      obj.gender = passenger.gender;\n      obj.berth = passenger.berth;\n      obj.mail_id = passenger.mail_id;\n      console.log(obj);\n      return obj;\n    }\n\n  },\n  watch: {\n    count: function (value) {\n      console.log('called');\n      this.passenger_details = [];\n\n      for (let i = 0; i < value; i++) {\n        this.passenger_details.push(this.Dummy({\n          train_no: '',\n          train_name: '',\n          DOJ: '',\n          name: '',\n          gender: '',\n          berth: '',\n          mail_id: ''\n        }));\n      }\n\n      console.log(this.passenger_details);\n      console.log('passenger created');\n    }\n  }\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,GAAE;AACF,WAAO;AACHC,gBAAU,EAACC,IAAI,CAACC,KAAL,CAAW,KAAKC,MAAL,CAAYC,MAAZ,CAAmBJ,UAA9B,CADR;AAEHK,WAAK,EAAC,CAFH;AAGHC,SAAG,EAAC,EAHD;AA